{
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "declaration": true,
        "noImplicitAny": false,
        "outFile": "built/index.js",
        "sourceMap": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "bower/*": [
                "./bower_components/*"
            ]
        }
    },
    "exclude": [
        "bower_components",
        "built"
    ]
}